{
  "name": "bomacronode",
  "displayName": "boMacro Node.js",
  "description": "Execution d'un module node.js en Macro.",
  "version": "1.6.0",
  "icon": "ressource/icone.png",
  "publisher": "borakLeRouge",
  "repository": {
    "type": "git",
    "url": "https://github.com/BorakLeRouge/boMacroNodeJs"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./ressource/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bomacronode.macroNode",
        "title": "Macro Node.js"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "bomacronode.macroNode",
          "when": "resourceLangId == javascript",
          "group": "0_Borak@1"
        }
      ],
      "commandPalette": [
        {
          "command": "bomacronode.macroNode",
          "when": "resourceLangId == javascript"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "iconv-lite": "^0.7.0"
  }
}
